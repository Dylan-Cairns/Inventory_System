                     <children>
                        <Button fx:id="addButton" mnemonicParsing="false" text="Add" />
                     </children>
                  </HBox>
                  <TableView fx:id="deleteProductTableview" prefHeight="200.0" prefWidth="200.0">
                     <columns>
                        <TableColumn fx:id="deleteProductTableviewPartIdCol" prefWidth="75.0" text="Part ID" />
